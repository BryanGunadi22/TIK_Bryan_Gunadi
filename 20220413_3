#include <iostream>
using namespace std;

//nested if = percabangan bersarang = if di dalam if//

int main()

{
    int nilai;
    
    {
    if (nilai % 2 == 0);
    cout <<"Bilangan Genap";
    }
    
    {
    if (nilai < 10);
    cout << "Bilangan Genap kurang dari 10";
    }
    
    else if
    {
    cout<< "bilangan Genap lebih dari  10"}
    }
    
    else if
    
    cout<<"Bilangan Ganjil";
    if (nilai <10)
    
    {
        cout << "Bilangan Ganjil kurang dari 10";
    }
    
    else if
    {
        cout<< "bilangan Ganjil ganjil dari  10"}
    }
    return 0;
}
