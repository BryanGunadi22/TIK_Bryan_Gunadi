#include <iostream>
#include <math.h>
using namespace std; 

int main()
{
    //std::array
    
    int angka [] = {
    };
    for (int j = 0; j<2; j++){
        cout<<"Masukkan angka = "; cin>>angka [j];
    }
    
    for (int i = 0 ; i<2; i ++){
        cout<<"Angka ke - " <<i<<" = " <<angka [i]*angka[i] << endl;
    }
    return 0;
}
